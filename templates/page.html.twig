	{% if page.show_skins_menu %}
		{{ page.show_skins_menu |raw }}
	{% endif %}

	{% if page.headline %}
		<section id="headline" class="headline">
			<div class="container">
				<a id="btn-second-menu" class="navbar-toggle collapsed" href="#menu-toggle">
					<i class="fa fa-bars" aria-hidden="true"></i>
				</a>
				{{ page.headline }}
			</div>
		</section>
	{% endif %}

	<header id="header" class="header">
		<div class="container">
			{% if logo %}
				<a id="logo" href="{{ front_page }}" title="{{ 'Home'|t }}" rel="home">
					<img src="{{ logo }}" alt="{{ 'Home'|t }}"/>
				</a>
			{% endif %}

			{% if site_name or site_slogan %}
				<div
					class="name-and-slogan">

					{# Use h1 when the content title is empty #}
					{% if title %}
						<strong class="site-name">
							<a href="{{ front_page }}" title="{{ 'Home'|t }}" rel="home">{{ site_name }}</a>
						</strong>
					{% else %}
						<h1 class="site-name">
							<a href="{{ front_page }}" title="{{ 'Home'|t }}" rel="home">{{ site_name }}</a>
						</h1>
					{% endif %}

					{% if site_slogan %}
						<div class="site-slogan">{{ site_slogan }}</div>
					{% endif %}
				</div>
			{% endif %}

			{{ page.header }}

		</div>
	</header>

	<div id="primary-menu" class="container">
		<a id="btn-main-menu" class="navbar-toggle collapsed" href="#main-menu-toggle">
			<i class="fa fa-bars" aria-hidden="true"></i>
		</a>
	</div>

	{% if page.primary_menu %}
		<section id="primary-main-menu" class="primary-main-menu">
			<div class="container">
				{{ page.primary_menu }}
			</div>
		</section>
	{% endif %}


	{% if page.secondary_menu %}
		<section id="secondary-main-menu" class="secondary-main-menu">
			<div class="container">
				{{ page.secondary_menu }}
			</div>
		</section>
	{% endif %}


	{% if page.slideshow %}
		<section id="slideshow" class="slideshow">
			<div class="container-fluid">
				{{ page.slideshow }}
			</div>
		</section>
	{% endif %}

	{% if page.featured %}
		<section id="featured" class="featured">
			<div class="container">
				{{ page.featured }}
			</div>
		</section>
	{% endif %}


	{% if page.highlighted %}
		<section id="hightlight" class="hightlight">
			<div class="container">
				{{ page.highlighted }}
			</div>
		</section>
	{% endif %}

	<section id="main" class="main">
		<a id="main-content"></a>
		<div class="container">
			<div class="row">

				<div id="main-content" class="content col-lg-{{ page.main_content_width }} col-md-{{ page.main_content_width }}">
					{% if tabs %}
						<nav class="tabs" role="navigation" aria-label="{{ 'Tabs'|t }}">
							{{ tabs }}
						</nav>
					{% endif %}
					{{ page.help }}

					{{ title_prefix }}
					{% if title %}
						<h1>{{ title }}</h1>
					{% endif %}
					{{ title_suffix }}

					{% if action_links %}
						<ul class="action-links">
							{{ action_links }}
						</ul>
					{% endif %}
					{{ page.content }}
				</div>
				{% if page.show_sidebar_first %}
					{% if page.sidebar_first %}
						<aside class="sidebar-first sidebar col-lg-{{ page.sidebar_first_width }} col-md-{{ page.sidebar_first_width }} col-sm-12 col-xs-12" role="complementary">
							{{ page.sidebar_first }}
						</aside>
					{% endif %}
				{% endif %}
			</div>
		</div>
	</section>

	{% if page.panel_first %}
		<section id="panel_first" class="panel_first">
			<div class="container-fluid">
				{{ page.panel_first }}
			</div>
		</section>
	{% endif %}

	{% if page.panel_second %}
		<section id="panel_second" class="panel_second">
			<div class="container">
				{{ page.panel_second }}
			</div>
		</section>
	{% endif %}

	{% if page.panel_third %}
		<section id="panel_third" class="panel_third">
			<div class="container">
				{{ page.panel_third }}
			</div>
		</section>
	{% endif %}


	{% if page.footer_firstcolumn or page.footer_secondcolumn or page.footer_thirdcolumn %}
		<section id="footer-columns" class="footer-columns">
			<div class="container">
				<div class="row">
					<div class="col-md-4">
						{{ page.footer_firstcolumn }}
					</div>
					<div class="col-md-4">
						{{ page.footer_secondcolumn }}
					</div>
					<div class="col-md-4">
						{{ page.footer_thirdcolumn }}
					</div>
				</div>
			</div>

		</section>
		<!-- /#footer-columns -->
	{% endif %}

	{% if page.footer %}
		<footer id="footer" role="contentinfo">
			<div class="container">
				{{ page.footer }}
			</div>
		</footer>
	{% endif %}
	<!--<a href="#" class="btn-btt">Volver arriba</a>-->
</div>
